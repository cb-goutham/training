    </servlet>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    
    <servlet>   
        <servlet-name>deactivate</servlet-name>
        <jsp-file>/JSP/deactivate.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>deactivate</servlet-name>
        <url-pattern>/JSP/deactivate</url-pattern>
    </servlet-mapping>
    
    
    
    
</web-app>
